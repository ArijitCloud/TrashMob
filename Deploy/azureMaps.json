{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "accounts_map_trashmobdev_name": {
            "defaultValue": "map_trashmobdev",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Maps/accounts",
            "apiVersion": "2021-02-01",
            "name": "[parameters('accounts_map_trashmobdev_name')]",
            "location": "global",
            "sku": {
                "name": "S0",
                "tier": "Standard"
            },
            "kind": "Gen1",
            "properties": {
                "disableLocalAuth": false
            }
        }
    ]
}